- content_for(:title) { "Reports" }

%table
  %thead
    %tr
      %th Source
      %th Status
      %th Weather
      %th Date
  %tbody
    - @reports.each do |report| 
      %tr
        %td=h report.source.name
        %td= report.status
        %td= report.weather
        %td= report.created_at.to_s(:short)
